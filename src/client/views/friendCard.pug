- var className = 'friend-card';
- if (friends) {className = 'friend-card-static';}
div(class=className)
  img(src=photo, alt=alt)
  .friend-info
    p #{name}
    if compatibility
      p #{compatibility}% compatibility!
      a#friends-link(href='#') View all friends!
    else
      ul
        each val, i in scores
          li Question#{i + 1}: #{val}
